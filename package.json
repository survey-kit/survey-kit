{
  "name": "survey-kit",
  "version": "1.0.0",
  "type": "module",
  "description": "Mobile-first bite-sized survey framework",
  "private": true,
  "workspaces": [
    "packages/registry",
    "packages/core",
    "packages/template"
  ],
  "scripts": {
    "dev": "npm run dev --workspaces",
