                {
                  "id": "delivery_manager",
                  "type": "email",
                  "label": "Delivery Manager email",
                  "placeholder": "delivery.manager@survey-kit.com",
                  "description": "Primary delivery/project manager",
                  "required": true,
                  "requiredToNavigate": true
