            {
              "id": "page-stage",
              "title": "Development Stage",
              "icon": "GitBranch",
              "description": "What stage is your project in?",
              "questions": [
                {
                  "id": "stage",
                  "type": "radio",
                  "label": "What is the current stage of your project?",
