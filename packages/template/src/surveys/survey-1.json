                  "requiredToNavigate": true,
                  "options": [
                    {
                      "label": "Discovery",
                      "value": "discovery"
                    },
                    {
                      "label": "Alpha",
                      "value": "alpha"
                    },
                    {
                      "label": "Beta",
                      "value": "beta"
                    },
                    {
                      "label": "Development",
                      "value": "development"
                    },
                    {
                      "label": "Live / Production",
                      "value": "production"
                    },
                    {
                      "label": "Retirement",
                      "value": "retirement"
                    }
