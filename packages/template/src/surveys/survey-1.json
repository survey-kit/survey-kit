{
  "id": "technology-inventory-survey",
  "title": "Technology Inventory",
  "description": "Complete information about your project's technology stack",
  "showQuestionNumbers": true,
  "navigation": {
    "stageOrder": "sequential",
    "groupOrder": "sequential",
    "pageOrder": "sequential"
