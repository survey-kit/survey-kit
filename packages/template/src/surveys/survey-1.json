            {
              "id": "page-project-name",
              "title": "Project Name",
              "icon": "Tag",
              "description": "What is your project called?",
              "questions": [
                {
                  "id": "project_name",
                  "type": "text",
                  "label": "What is the full name of your project?",
                  "placeholder": "e.g., Survey Kit",
                  "required": true,
                  "requiredToNavigate": true,
                  "validation": [
                    {
                      "type": "min",
                      "value": 3,
                      "message": "Project name must be at least 3 characters"
                    }
                  ]
                },
                {
                  "id": "project_short",
                  "type": "text",
                  "label": "Project short name or acronym",
                  "placeholder": "e.g., SK",
                  "description": "A short identifier for your project",
                  "required": true,
                  "requiredToNavigate": true
                }
              ]
            },
            {
              "id": "page-programme",
              "title": "Programme",
              "icon": "Briefcase",
              "description": "Which programme does this project belong to?",
              "questions": [
                {
                  "id": "programme_name",
                  "type": "text",
                  "label": "Programme name",
                  "placeholder": "e.g., Survey Kit Programme",
                  "required": true,
                  "requiredToNavigate": true
                },
                {
                  "id": "programme_short",
                  "type": "text",
                  "label": "Programme short name or acronym",
                  "placeholder": "e.g., SKP",
                  "required": false
                }
              ]
            },
            {
              "id": "page-description",
              "title": "Description",
              "icon": "AlignLeft",
              "description": "Describe your project",
              "questions": [
                {
                  "id": "description",
                  "type": "textarea",
                  "label": "Project description",
                  "placeholder": "Briefly describe what this project does...",
                  "description": "Provide a brief overview of the project's purpose and goals",
                  "required": true,
                  "requiredToNavigate": true
                }
              ]
            }
          ]
        },
        {
          "id": "group-project-status",
          "title": "Project Status",
          "description": "Current status and ownership",
