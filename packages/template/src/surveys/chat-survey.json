{
    "id": "chat-demo",
    "title": "Technology Survey",
    "stages": [
        {
            "id": "stage-1",
            "title": "Your Preferences",
            "groups": [
                {
                    "id": "group-1",
                    "title": "Technology",
                    "pages": [
                        {
                            "id": "page-name",
                            "questions": [
                                {
                                    "id": "name",
                                    "type": "text",
                                    "label": "Hello! What's your name?",
                                    "placeholder": "Enter your name",
                                    "requiredToNavigate": true
                                }
                            ]
                        },
                        {
                            "id": "page-tech",
                            "questions": [
                                {
                                    "id": "favourite_tech",
                                    "type": "text",
                                    "label": "What is your favourite technology?",
                                    "description": "This can be a programming language, framework, or tool.",
                                    "placeholder": "e.g., React, Python, AWS",
                                    "requiredToNavigate": true
                                }
                            ]
                        },
                        {
                            "id": "page-frontend",
                            "questions": [
                                {
                                    "id": "frontend_framework",
                                    "type": "radio",
                                    "label": "Pick your preferred frontend framework:",
                                    "options": [
                                        {
                                            "value": "react",
                                            "label": "React"
                                        },
                                        {
                                            "value": "vue",
                                            "label": "Vue"
                                        },
                                        {
                                            "value": "angular",
                                            "label": "Angular"
                                        },
                                        {
                                            "value": "svelte",
                                            "label": "Svelte"
                                        },
                                        {
                                            "value": "other",
                                            "label": "Other"
                                        }
                                    ],
                                    "requiredToNavigate": true
                                }
                            ]
                        },
                        {
                            "id": "page-tools",
                            "questions": [
                                {
                                    "id": "dev_tools",
                                    "type": "checkbox",
                                    "label": "Which development tools do you use regularly?",
                                    "description": "Select all that apply.",
                                    "options": [
                                        {
                                            "value": "vscode",
                                            "label": "VS Code"
                                        },
                                        {
                                            "value": "jetbrains",
                                            "label": "JetBrains IDE"
                                        },
                                        {
                                            "value": "github",
                                            "label": "GitHub"
                                        },
                                        {
                                            "value": "gitlab",
                                            "label": "GitLab"
                                        },
                                        {
                                            "value": "docker",
                                            "label": "Docker"
                                        },
                                        {
                                            "value": "figma",
                                            "label": "Figma"
                                        }
                                    ],
                                    "requiredToNavigate": true
                                }
                            ]
                        },
                        {
                            "id": "page-experience",
                            "questions": [
                                {
                                    "id": "experience_years",
                                    "type": "radio",
                                    "label": "How many years of development experience do you have?",
                                    "options": [
                                        {
                                            "value": "0-1",
                                            "label": "Less than 1 year"
                                        },
                                        {
                                            "value": "1-3",
                                            "label": "1-3 years"
                                        },
                                        {
                                            "value": "3-5",
                                            "label": "3-5 years"
                                        },
                                        {
                                            "value": "5-10",
                                            "label": "5-10 years"
                                        },
                                        {
                                            "value": "10+",
                                            "label": "10+ years"
                                        }
                                    ],
                                    "requiredToNavigate": true
                                }
                            ]
                        },
                        {
                            "id": "page-feedback",
                            "questions": [
                                {
                                    "id": "feedback",
                                    "type": "text",
                                    "label": "Any final thoughts or feedback?",
                                    "placeholder": "Optional feedback...",
                                    "required": false
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}