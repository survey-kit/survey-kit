# AWS Configuration
aws_profile = "your-aws-profile-name"
aws_region  = "us-east-1"

# S3 Bucket Configuration
bucket_name = "survey-kit-template-dev"

# Domain Configuration
domain_name = "aws-template.survey-kit.com"

# Environment
environment = "dev"

# ACM Certificate ARN (must be in us-east-1 for CloudFront)
# Create this in AWS Certificate Manager with DNS validation
# Example: arn:aws:acm:us-east-1:123456789012:certificate/12345678-1234-1234-1234-123456789012
acm_certificate_arn = "arn:aws:acm:us-east-1:YOUR_ACCOUNT_ID:certificate/YOUR_CERTIFICATE_ID"
